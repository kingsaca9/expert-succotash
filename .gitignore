function trackPackage() {
    var trackingNumber = document.getElementById("trackingNumber").value;
    var resultDiv = document.getElementById("result");
    if (
        trackingNumber === "ISRTX-0035-5537-33AB" ||
        trackingNumber === "ISRTX-0021-7622-21AU" ||
        trackingNumber === "ISRTX-0078-4521-99XY" ||
        trackingNumber === "ISRTX-8742-1934-56XY" ||
        trackingNumber === "ISRTX-6529-7081-33AB" ||
        trackingNumber === "ISRTX-9103-2845-77CD" ||
        trackingNumber === "ISRTX-3478-6290-12EF" ||
        trackingNumber === "ISRTX-5821-4756-99GH"
    ) {
        resultDiv.innerHTML = "Status: Pending (Out for Delivery)";
    } else {
        resultDiv.innerHTML = "Invalid Tracking Number";
    }
}